mixin test(name, title)
	.test(id="test-"+name)
		.test__title= title
		.test__list
			.test__item.test__1920.diff-hidden 
				.test__item-head
					.test__item-width 1920px 
				
				.test__item-body
					.test__item-iframe-wrap 
						iframe(width="1920" scrolling="no"    src="_test-"+name+".html" frameborder="0" ).test__item-iframe 
					img(src="test/img/"+name+"-1920.webp", alt="").test__item-maket.img1._lock
		
			
			.test__item.test__1366.diff-hidden 
				.test__item-head
					.test__item-width 1366px 
			
				.test__item-body
					.test__item-iframe-wrap
						iframe(width="1366" scrolling="no"   src="_test-"+name+".html"  frameborder="0").test__item-iframe 
					img(src="test/img/"+name+"-1366.webp", alt="").test__item-maket.img2._lock
					//- canvas.test__item-diff
			.test__item.test__768.diff-hidden 
				.test__item-head
					.test__item-width 768px 
				.test__item-body
					.test__item-iframe-wrap
						iframe(width="768" scrolling="no"   src="_test-"+name+".html"  frameborder="0").test__item-iframe 
					img(src="test/img/"+name+"-768.webp", alt="").test__item-maket._lock
					//- canvas.test__item-diff
			.test__item.test__360.diff-hidden   
				.test__item-head
					.test__item-width 360px 
				.test__item-body
					.test__item-iframe-wrap
						iframe(width="360" scrolling="no"   src="_test-"+name+".html"  frameborder="0").test__item-iframe
					img(src="test/img/"+name+"-360.webp", alt="").test__item-maket._lock
					//- canvas.test__item-diff